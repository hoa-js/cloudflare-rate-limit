{
  "name": "@hoajs/cloudflare-rate-limit",
  "version": "0.2.0",
  "description": "Cloudflare rate limiter middleware for Hoa.",
  "main": "./dist/cjs/index.js",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "CHANGELOG.md"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsup",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "husky"
  },
  "author": "nswbmw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoa-js/cloudflare-rate-limit.git"
  },
  "keywords": [
    "hoa",
    "middleware",
    "cloudflare",
    "kv",
    "ratelimit",
    "ratelimiter",
    "rate-limit",
    "limiter"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "eslint": "9.39.1",
    "globals": "16.5.0",
    "hoa": "*",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "neostandard": "0.12.2",
    "tsup": "8.5.0"
  },
  "peerDependencies": {
    "hoa": "*"
  },
  "dependencies": {
    "cloudflare-kv-rate-limit": "~2.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
